<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jemiola.moodtimeline">

  <application
      android:name="com.jemiola.moodtimeline.base.BaseApplication"
      android:allowBackup="true"
      android:icon="@mipmap/app_round_logo"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/app_round_logo"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name="com.jemiola.moodtimeline.views.splash.SplashActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="com.jemiola.moodtimeline.views.navigation.NavigationActivity"/>
  </application>

</manifest>